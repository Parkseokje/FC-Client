<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">

  <link rel="stylesheet" href="/static/stylesheets/reset.css" />
  <link rel="stylesheet" href="/static/stylesheets/bootstrap-iso.css" />
  <!--<link rel="stylesheet" href="/static/stylesheets/bootstrap.min.css" />-->
  <link rel="stylesheet" href="/static/stylesheets/layout.css" />
  <link rel="stylesheet" href="/static/stylesheets/skins.css" />
  <link rel="stylesheet" href="/static/stylesheets/star-rating-svg.css" />
  <link rel="stylesheet" href="/static/stylesheets/font-awesome.min.css" />
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/animate.css@3.5.2/animate.min.css">
  <link rel="stylesheet" href="/static/stylesheets/nplayer.css">
  {{!-- <link rel="stylesheet" href="/static/stylesheets/nplayer_res.css"> --}}

  <!-- favicon -->
  <link rel="apple-touch-icon" sizes="180x180" href="/static/images/favicon/{{corp}}/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/static/images/favicon/{{corp}}/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/static/images/favicon/{{corp}}/favicon-16x16.png">
  <link rel="manifest" href="/static/images/favicon/{{corp}}/site.webmanifest">
  <link rel="mask-icon" href="/static/images/favicon/{{corp}}/safari-pinned-tab.svg" color="#5bbad5">
  <meta name="msapplication-TileColor" content="#da532c">
  <meta name="theme-color" content="#ffffff">

  <title>{{logoName}}</title>
</head>

<body oncontextmenu="return false" ondragstart="return false" onselectstart="return false" class="body_bg_01 {{theme}}">
  <div class="wrapper">
    {{#if (isEquals current_path 'login')}}
    {{{body}}}
    {{else if (isEquals current_path 'quickicon')}}
    {{{body}}}
    {{else}}
    {{> left_nav}}
    {{#if (isEquals group_path 'education') }}
    {{/if}}
    {{{body}}}
    {{/if}}
  </div>

  <script src="/static/vendor/require.2.3.2.js"></script>
  <script src="/static/javascripts/app.js"></script>

  {{#if (isEquals current_path 'login')}}
  <script src="/static/javascripts/login.js"></script>
  {{else}}
  {{#ifCond current_path '!==' 'login'}}
  <script src="/static/javascripts/left_nav.js"></script>{{/ifCond}}
  {{#if (isEquals group_path 'education') }}
  <script src="/static/javascripts/education.js"></script>{{/if}}
  {{#if (isEquals current_path 'course') }}
  <script src="/static/javascripts/course.js"></script>{{/if}}
  {{#if (isEquals current_path 'quiz')}}
  <script src="/static/javascripts/quiz.js"></script>{{/if}}
  {{#if (isEquals current_path 'checklist')}}
  <script src="/static/javascripts/checklist.js"></script>{{/if}}
  {{#if (isEquals current_path 'vimeo')}}
  <script src="/static/javascripts/video.js"></script>{{/if}}
  {{#if (isEquals current_path 'aqua-mobile')}}
  <script src="/static/javascripts/video_aqua.js"></script>{{/if}}
  {{#if (isEquals current_path 'aqua-desktop')}}
  <script src="/static/javascripts/video_aqua_pc.js"></script>{{/if}}
  {{#if (isEquals current_path 'evaluate')}}
  <script src="/static/javascripts/evaluate.js"></script>{{/if}}
  {{#if (isEquals current_path 'complete')}}
  <script src="/static/javascripts/complete.js"></script>{{/if}}
  {{#if (isEquals current_path 'profile')}}
  <script src="/static/javascripts/profile.js"></script>{{/if}}
  {{#if (isEquals current_path 'quickicon')}}
  <script src="/static/javascripts/video_quick_icon.js"></script>{{/if}}
  {{#if (isEquals current_path 'help')}}
  <script src="/static/javascripts/help.js"></script>{{/if}}
  {{/if}}

  <script>
    (function (i, s, o, g, r, a, m) {
      i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
        (i[r].q = i[r].q || []).push(arguments)
      }, i[r].l = 1 * new Date(); a = s.createElement(o),
        m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
    })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

    ga('create', 'UA-93048897-1', 'auto');
    ga('send', 'pageview');

  </script>

</body>

</html>