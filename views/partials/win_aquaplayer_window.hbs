<div id="video" class="wrapper_video" data-url="{{video_url}}" data-watermark="{{watermark}}">
  <div class="wrapper_foot">
    <div class="NPlyaer_skin01">
      <div class="area_seek">
        <div class="track_back">
          <div class="track_seekBtn" style="left:0%;">
            <a class="btn_common btn_seek">
              <img src="/images/aquaplayer/blank.gif" width="100%" height="100%" border="0"></img>
              </span>
            </a>
          </div>
          <div class="track_repeatBtnA" style="left:0%;display:none;">
            <a class="btn_common btn_repeatSet">
              <img src="/images/aquaplayer/blank.gif" width="100%" height="100%" border="0">
              </span>
            </a>
          </div>
          <div class="track_repeatBtnB" style="left:100%;display:none;">
            <a class="btn_common btn_repeatSet">
              <img src="/images/aquaplayer/blank.gif" width="100%" height="100%" border="0">
              </span>
            </a>
          </div>
          <div class="track_repeat" style="left:0%; width:100%;display:none;"></div>
          <div class="track_current" style="width:0%;"></div>
        </div>
      </div>
      <div class="area_control">
        <div class="contro_l">
          <div class="control_play">
            <a class="btn_common np_btn_play">
              </span>
            </a>
          </div>
          <div class="control_text_status">Closed</div>
          <div class="control_text_time">
            <strong id="text_currentTime">00:00:00</strong> /
            <span id="text_duration">00:00:00</span>
          </div>
        </div>
        <div class="contro_r">
          <div class="control_volume">
            <div class="sound_btn">
              <a class="btn_common btn_sound">
                </span>
              </a>
            </div>
            <div class="sound_track_back">
              <div class="sound_track_cur" style="width:100%;">
                <img src="/images/aquaplayer/blank.gif" width="100%" height="100%" border="0">
              </div>
            </div>
          </div>
          <div class="control_fullScr">
            <a class="btn_common btn_fullScr">
              </span>
            </a>
          </div>
          <div class="control_repeat">
            <a class="btn_common btn_repeat">
              </span>
            </a>
          </div>
          <div class="control_bar"></div>
          <div class="control_speed">
            <ul>
              <li>
                <a class="btn_common btn_s06">
                  </span>
                </a>
              </li>
              <li>
                <a class="btn_common btn_s07_dot">
                  </span>
                </a>
              </li>
              <li>
                <a class="btn_common btn_s08">
                  </span>
                </a>
              </li>
              <li>
                <a class="btn_common btn_s09_dot">
                  </span>
                </a>
              </li>
              <li>
                <a class="btn_common btn_s10">
                  </span>
                </a>
              </li>
              <li>
                <a class="btn_common btn_s11_dot">
                  </span>
                </a>
              </li>
              <li>
                <a class="btn_common btn_s12">
                  </span>
                </a>
              </li>
              <li>
                <a class="btn_common btn_s13_dot">
                  </span>
                </a>
              </li>
              <li>
                <a class="btn_common btn_s14">
                  </span>
                </a>
              </li>
              <li>
                <a class="btn_common btn_s15_dot">
                  </span>
                </a>
              </li>
              <li>
                <a class="btn_common btn_s16">
                  </span>
                </a>
              </li>
              <li>
                <a class="btn_common btn_s17_dot">
                  </span>
                </a>
              </li>
              <li>
                <a class="btn_common btn_s18">
                  </span>
                </a>
              </li>
              <li>
                <a class="btn_common btn_s19_dot">
                  </span>
                </a>
              </li>
              <li>
                <a class="btn_common btn_s20">
                  </span>
                </a>
              </li>
              <!-- _cur 을 붙이면 현재 스피드 체크 -->
            </ul>
          </div>
          <div class="control_bar"></div>
          <div class="control_seek">
            <ul>
              <li>
                <a class="btn_common btn_seekPrev">
                  </span>
                </a>
              </li>
              <li class="inputArea">
                <input type="text" class="input_setSeekTime" value="30" />
              </li>
              <li>
                <a class="btn_common btn_seekNext">
                  </span>
                </a>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>